<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
	<script type="text/javascript" src="/xy/easyui/jquery.min.js"></script>
    <script type="text/javascript" src="/xy/easyui/jquery.easyui.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/xy/easyui/themes/default/easyui.css">
    <link rel="stylesheet" type="text/css" href="/xy/easyui/themes/icon.css">
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>添加商品</title>
</head>
<body>
	<table>
		<tr>
			<td colspan="2">添加商品</td>		
		</tr>
		<tr>
			<td>
				商品名称：
			<td>
			<td>
				<input name="name" class="easyui-textbox"/>
			</td>		
		</tr>
		<tr>
			<td>
				商品价格：
			<td>
			<td>
				<input name="price" class="easyui-textbox"/>
			</td>		
		</tr>
		<tr>
			<td>
				商品所在仓库
			<td>
			<td>
				<input name="storage" class="easyui-textbox"/>
			</td>		
		</tr>
			<td>
				商家
			<td>
			<td>
				<input name="bossid" class="easyui-textbox"/>
			</td>		
		</tr>
		<tr>
			<td>
				生产时间：
			<td>
			<td>
				<input name="name" class="easyui-datebox"/>
			</td>		
		</tr>
		<tr>
			<td>
				商品描述信息：
			<td>
			<td>
				<input name="name" class="easyui-datebox"/>
			</td>		
		</tr>
		<tr>
			<td>
				商品图片：
			<td>
			<td>
				<input name="name" class="easyui-datebox"/>
			</td>		
		</tr>
		<tr>
			<td>
				生产地址：
			<td>
			<td>
				<input name="name" class="easyui-datebox"/>
			</td>		
		</tr>
		<tr>
			<td>
				备注
			<td>
			<td>
				<input name="name" class="easyui-datebox"/>
			</td>		
		</tr>
	</table>
</body>
</html>